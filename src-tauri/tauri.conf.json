{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "Custori",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "fr.indiecog.custori",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoint": [
        
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEMyOEU2QzUyQ0VDRDc0MQpSV1JCMSt3c3hlWW9ETER6cGdPMnRFazJUNkE4ODBXSms2SUhMalFIMWJyeFdCcGpOREZHMFFYbwo="
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Custori",
        "width": 800,
        "height": 600
      }
    ]
  }
}
